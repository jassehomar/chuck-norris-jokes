{
  "name": "jassehomar/chuck-norris-jokes",
  "description": "Create random Chuck Norris Jokes.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Omar Jasseh",
      "email": "jassehomar99@gmail.comm"
    }
  ],
  "require": {
    "guzzlehttp/guzzle": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "JassehOmar\\ChuckNorrisJokes\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JassehOmar\\ChuckNorrisJokes\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3",
    "orchestra/testbench": "~3.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "JassehOmar\\ChuckNorrisJokes\\ChuckNorrisJokesServiceProvider"
      ],
      "aliases": {
        "ChuckNorris": "JassehOmar\\ChuckNorrisJokes\\Facades\\ChuckNorris"
      }
    }
  }
}
